root ROOT {
  expected: {question, subject, difficulty}
}

data MATHD {
  id: mathd/train
  hash: eb74dd955868e34f397da411235a74c1ba5690d26851af481979fe9edee1cf94
}

code RETRIEVAL {
  code:
```
_fun = (env) => {
  let s = env['state']['ROOT']['subject'];
  let d = env['state']['ROOT']['difficulty'];
  return env['state']['MATHD'].filter((r) => (r['subject] == s && r['difficulty'] == d))[:4];
};
```
}

map LOOP {
  from: ROOT
  repeat: 20
}

llm MODEL {
  max_tokens: 1024
  temperature: 0.7
  few_shot_count: 4
  few_shot_prompt:
```
QUESTION: ${RETRIEVAL.question}
EXPLANATION: ${RETRIEVAL.explanation}
ANSWER: ${RETRIEVAL.answer}

```
  prompt:
```
QUESTION: ${MAP.question}
```
  stop:
```
QUESTION:
```
}

code POSTPROCESS {
  code:
```
_fun = (env) => {
  let completion = env['state']['MODEL']['completion']['text'];
  return {
    "explanation": completion["text"].split("EXPLANATION:")[1].split("\nANSWER:")[0].trim(),
    "answer": completion["text"].split("ANSWER:")[1].trim(),
  };
}
```
}

reduce LOOP {
  from: POSTPROCESS
}

code CONSENSUS {
  code:
```
_fun = (env) => {
  let counts = {};
  for d in env['state']['REPEAT'] {
    if d['answer'] not in counts:
      counts[d['answer']] = 0
    counts[d['answer']] += 1
  }
  let answer = env['state']['REPEAT'][0]['answer'];
  let max_count = 0;
  for a in counts.keys() {
    if max_count < counts[a] {
      max_count = counts[a];
      answer = a;
    }
  }
  return { "answer": answer };
};
```
}
