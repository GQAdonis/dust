root ROOT {
  expected: {question, subject, difficulty}
}

data MATHD {
  id: mathd/train
  expected: []{question, subject, difficulty, explanation, anwer}
}

code RETRIEVE {
  code:
```
_block = (env) => {
  let s = env['state']['ROOT']['subject'];
  let d = env['state']['ROOT']['difficulty'];
  return env['state']['MATHD'].filter((r) => (r['subject] == s && r['difficulty'] == d))
};
```
  expected: []{question, explanation, answer}
}

llm MODEL {
  examples_count: max
  examples_prompt:
```
QUESTION: ${RETRIEVE.question}
EXPLANATION: ${RETRIEVE.explanation}
ANSWER: ${RETRIEVE.answer}
```
  prompt:
```
QUESTION: ${ROOT.question}
```
  extract:
```
EXPLANATION: ${explanation}
ANSWER: ${answer}
```
  expected: {explanation, answer}
}

repeat REPEAT {
  blocks: MODEL
  count: 20
  expected: []{explanation, answer}
}

code CONSENSUS {
  code:
```
_block = (env) => {
  let counts = {};
  for d in env['state']['REPEAT'] {
    if d['answer'] not in counts:
      counts[d['answer']] = 0
    counts[d['answer']] += 1
  }
  let answer = env['state']['REPEAT'][0]['answer'];
  let max_count = 0;
  for a in counts.keys() {
    if max_count < counts[a] {
      max_count = counts[a];
      answer = a;
    }
  }
  return { "answer": answer };
};
```
  expected: {answer}
}
